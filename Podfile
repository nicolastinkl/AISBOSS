pod 'SnapKit', '~> 0.18.0'

pod 'AIAlertView', :git => 'https://github.com/asiainfomobile/AIAlertView.git'

pod 'YYImage', '~> 1.0.1'

# iOS System Facebook

pod 'AsyncDisplayKit', '~> 1.9.4'

# iOS Debug

pod 'Igor', '~> 0.5.0'

end
